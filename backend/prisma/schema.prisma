// Prisma schema

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LotteryResult {
  id        Int      @id @default(autoincrement())
  city      String
  drawDate  DateTime
  numbers   String
  createdAt DateTime @default(now())

  @@unique([city, drawDate], name: "city_drawDate")
}
